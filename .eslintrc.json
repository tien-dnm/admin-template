{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "plugin:react-hooks/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "react-hooks"],
	"rules": {
		"react/prop-types": "off",
		"no-unused-vars": "warn",
		"react/display-name": "off",
		"react/no-unescaped-entities": "off"
	}
}
